{
  "name": "vscode-rpc",
  "displayName": "Vscode RPC",
  "description": "Discord RPC so other know if you are coding and what language it is. :D",
  "version": "0.1.4",
  "publisher": "Frank1o3",
  "keywords": ["vscode", "discord", "rpc", "presence", "coding", "developer status"],
  "author": {
    "name": "Frank1o3",
    "email": "jahdy1o3@gamil.com"
  },
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "url": "github.com/Frank1o3/vscode-rpc"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/discord-rpc": "^4.0.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  },
  "contributes": {
    "configuration": {
      "title": "Discord Presence",
      "properties": {
        "discordPresence.clientId": {
          "type": "string",
          "default": "",
          "description": "Discord Application Client ID"
        },
        "discordPresence.ClientSecret": {
          "type": "string",
          "default": "",
          "description": "ClientSecret optional to se btw"
        },
        "discordPresence.showWorkspace": {
          "type": "boolean",
          "default": true,
          "description": "Show workspace name in Discord presence"
        },
        "discordPresence.transport": {
          "type": "string",
          "enum": [
            "ipc",
            "websocket"
          ],
          "default": "ipc",
          "description": "Transport method for Discord RPC (use 'websocket' for remote environments)"
        },
        "discordPresence.debounceMs": {
          "type": "number",
          "default": 1000,
          "description": "Debounce time (ms) for presence updates"
        }
      }
    }
  },
  "dependencies": {
    "discord-rpc": "^4.0.1"
  }
}
