{
  "name": "vscode-rpc",
  "displayName": "Vscode RPC",
  "description": "Discord RPC so other know if you are coding and what language it is. :D",
  "version": "0.1.2",
  "publisher": "Frank1o3",
  "author": {
    "name": "Frank1o3",
    "email": "jahdy1o3@gamil.com"
  },
  "engines": {
    "vscode": "^1.102.3"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "url": "github.com/Frank1o3/vscode-rpc"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/discord-rpc": "^4.0.9",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "discord-rpc": "^4.0.1",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2"
  },
  "contributes": {
    "configuration": {
      "title": "Discord Presence (Simple)",
      "properties": {
        "discordPresence.clientId": {
          "type": "string",
          "default": "",
          "description": "Discord Application Client ID"
        },
        "discordPresence.largeImageKey": {
          "type": "string",
          "default": "coding",
          "description": "Discord asset key for large image (upload one image with this key)."
        },
        "discordPresence.smallImageKey": {
          "type": "string",
          "default": "vscode",
          "description": "Discord asset key for small image (upload small image with this key)."
        },
        "discordPresence.showWorkspace": {
          "type": "boolean",
          "default": true,
          "description": "Append workspace folder name to presence details."
        }
      }
    }
  }
}